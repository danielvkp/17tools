{
   "name": "Aplicacion Para L7",
   "version": "1.1",
   "description": "Extension L7 tools",
   "manifest_version": 2,
   "content_scripts": [
     {
       "matches": ["https://l7.trade/trade"],
       "js": ["scripts/content_script.js"]
     }
   ],
   "browser_action": {
    "default_icon": "128.png",
    "default_popup": "popup.html"
  },
   "icons": {
     "128": "128.png"
   },
  "permissions": ["https://l7.trade/trade", "storage", "activeTab", "declarativeContent", "tabs"]
 }
